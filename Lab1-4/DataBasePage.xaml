<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Lab1_4.DataBasePage"
             Title="DataBase">
    
    <VerticalStackLayout>
        
        <Picker
            x:Name="picker"
            Title="Select a team"
            TextColor="Red"
            Background="blue">
            <Picker.ItemsSource>
                <x:Array Type="{x:Type x:String}"/>
            </Picker.ItemsSource>
        </Picker>

    </VerticalStackLayout>
    
</ContentPage>